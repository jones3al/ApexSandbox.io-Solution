public String ageGroup(Integer n) {
    //code here
    String groupAge;
        if (n <= 1){
            groupAge = 'Infant';
        }else
        if (n >=2 && n <=14){
            groupAge = 'Child';
        } else
        if (n >= 15 && n <= 21){
            groupAge = 'Youth';
        }
        else{
            groupAge = 'Adult';
        }
    return groupAge;
}