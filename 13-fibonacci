public Integer fibonacci(Integer n) {
    //code here
    Integer current = 0;
    Integer prev1 = 1;
    Integer prev2 = 1;

    if(n == 1 || n == 2){
        return 1;
    }
    for(Integer i = 3 ; i <= n; i++) { 
        current = prev1 + prev2;
        prev1 = prev2;
        prev2 = current;
        
        }

    return prev2;

}
