public String registerStudent(String name, String phone, String email) {
    // code here

    //@param String name is Name(string), String phone is apxio__Phone__c(string), String email is apxio__Email__c(string)
    //@return string apxio__Registration_Number__c (autogenerated)

    apxio__Student__c newStudent = new apxio__Student__c();
    newStudent.Name = name;
    newStudent.apxio__Email__c = email;
    newStudent.apxio__Phone__c = phone;

    insert newStudent;

    Id newStudentId = newStudent.Id;

    List<apxio__Student__c> regNumber = [SELECT apxio__Registration_Number__c FROM apxio__Student__c WHERE Id = :newStudentId];


    return regNumber[0].apxio__Registration_Number__c.toString();

    //
}