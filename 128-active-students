public List<apxio__Student__c> selectActiveStudents() {
    // code here

    //implement a method that reutns the list of all student records with active box checked
    //all records must have valid id and Name fields
    //@parameters none, return list of student records

    List<apxio__Student__c> students = [SELECT Id, Name FROM apxio__Student__c where apxio__Active__c = true];

    //loop through student records, if Name or Id is NOT null or empty add to return
    List<apxio__Student__c> validStudents = new List<apxio__Student__c>();

    for(apxio__Student__c record : students){

        if(record.Id != null && record.Name != null){

            validStudents.add(record);
        }
    }

    return validStudents;
}